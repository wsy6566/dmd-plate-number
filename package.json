{
  "name": "dmd-plate-number",
  "license": "MIT",
  "version": "0.0.2",
  "description": "Chinese vehicle plate number keyboard for React and Only 7 charts ,not have green plate",
  "author": "weisy",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "webpack",
    "build:watch": "webpack --watch --info-verbosity verbose",
    "build:prod": "rimraf dist && NODE_ENV=production webpack",
    "postversion": "git push && git push --tags && npm publish",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:fix": "tslint -p tsconfig.json -c tslint.json --fix",
    "prestart": "rimraf dist && tsc",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addon-viewport": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@svgr/webpack": "^4.1.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "husky": "^1.2.1",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "postcss-px-to-viewport": "^0.0.3",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.14.2",
    "react-dev-utils": "^10.0.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "sass": "^1.28.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^8.0.7",
    "tslint": "5.11.0",
    "typescript": "^4.0.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "yarn lint:fix",
      "git add"
    ]
  }
}
